{
  "name": "power-cell-tracker",
  "productName": "power-cell-tracker",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "NoamPrag",
    "email": "noam.prag07@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "power_cell_tracker"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "css-loader": "^4.3.0",
    "electron": "10.1.3",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "node-loader": "^1.0.2",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@devexpress/dx-react-chart": "^2.7.2",
    "@devexpress/dx-react-chart-material-ui": "^2.7.2",
    "@devexpress/dx-react-core": "^2.7.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/material-ui": "^0.21.8",
    "@types/react": "^16.9.53",
    "@types/react-chartjs-2": "^2.5.7",
    "@types/react-dom": "^16.9.8",
    "chart.js": "^2.9.4",
    "clsx": "^1.1.1",
    "electron-rebuild": "^2.3.5",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^16.14.0",
    "react-chartjs-2": "^2.11.1",
    "react-confetti": "^6.0.1",
    "react-countup": "^4.3.3",
    "react-dom": "^16.14.0",
    "react-use": "^17.1.1",
    "react-visibility-sensor": "^5.1.1",
    "serialport": "^9.0.7"
  }
}
